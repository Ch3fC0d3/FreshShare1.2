{
  "name": "freshshare",
  "version": "1.0.0",
  "description": "FreshShare application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "qa:serve": "node preview-server.js || node server.js",
    "qa:wait": "wait-on http://127.0.0.1:3000",
    "qa:links": "linkinator http://127.0.0.1:3000 --recurse --skip 'mailto:,tel:'",
    "qa:playwright": "playwright test",
    "qa": "start-server-and-test qa:serve http://127.0.0.1:3000 qa:playwright"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "basic-ftp": "^5.0.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dompurify": "^3.0.6",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.9.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.6",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.6",
    "snyk": "^1.1295.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.56.1",
    "jest": "^29.7.0",
    "linkinator": "^6.3.0",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1",
    "start-server-and-test": "^2.1.2",
    "supertest": "^6.3.4",
    "wait-on": "^8.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "verbose": true
  }
}
